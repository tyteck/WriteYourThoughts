{
    "name": "tyteck/write-your-thoughts",
    "description": "simple blog package appropriate for my blog needs",
    "version": "1.0.0-dev",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "frederick tyteca",
            "email": "frederick@tyteca.net"
        }
    ],
    "require": {},
    "autoload": {
        "psr-4": {
            "Tyteck\\WriteYourThoughts\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tyteck\\WriteYourThoughts\\Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "orchestra/testbench": "^6.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tyteck\\WriteYourThoughts\\ThoughtsServiceProvider"
            ],
            "aliases": {
                "Thoughts": "Tyteck\\WriteYourThoughts\\Facades\\Thoughts"
            }
        }
    }
}
